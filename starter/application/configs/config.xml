<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
<!-- #SCOPE_NCSU_PRIVATE -->
	<production>
		<debug>off</debug>
		<resources>
			<sample>
				<host>x</host>
			</sample>
		</resources>
		<sysAdmins>
			<admin>jthurteau</admin>
		</sysAdmins>
		<defaultBreadCrumbs>
			<link>
				<label>Home Page</label>
				<url>[[baseUrl]]</url>
			</link>
		</defaultBreadCrumbs>
		<includesPath>/sample/public/website/</includesPath>
		<additionalIncludeTags>0</additionalIncludeTags>
	</production>
	<development zf:extends="production">
	</development>
	<your-local-development zf:extends="development">
			<includesPath>/sample/website/</includesPath>
			<additionalIncludeTags>1</additionalIncludeTags>
	</your-local-development>
</config>

